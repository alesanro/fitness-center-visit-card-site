div.floating-menu
    position: fixed

div.floating-menu a, div.floating-menu table
    display: block

div.centered-menu
    background: orange
    width: 100%
    height: 40px
    padding-top: 10px

div.right-menu
    margin-right: 100px
    margin-top: 100px
    background: purple
    height: 400px
    width: 15%
    float: right

div.left-menu
    margin-left: 100px
    margin-top: 100px
    background: pink
    height: 500px
    width: 15%
    float: left

div.right-menu a, div.left-menu 
    font-size: 20px
    display: block

a.menuButton
    margin: 5px 10px 5px 10px

#nav
    padding: 0px
    width: 100%
    background: orange
    height: 40px
    padding-top: 15px

#nav-wrap
    height: 30px